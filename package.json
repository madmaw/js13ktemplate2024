  "main": "index.js",
  "name": "js13ktemplate2024",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "dist": "pnpm glslx:dist && pnpm build && pnpm roadroller && pnpm inline && pnpm zip",
    "glslx": "glslx glslx/overlay.glslx --output=gen/shaders/overlay.js --format=js --renaming=internal-only --pretty-print && glslx glslx/crt_bend.glslx --output=gen/shaders/crt_bend.js --format=js --renaming=internal-only --pretty-print && glslx glslx/crt_pixelate.glslx --output=gen/shaders/crt_pixelate.js --format=js --renaming=internal-only --pretty-print",
    "glslx:dist": "glslx glslx/overlay.glslx --output=gen/shaders/overlay.js --format=js && glslx glslx/crt_bend.glslx --output=gen/shaders/crt_bend.js --format=js && glslx glslx/crt_pixelate.glslx --output=gen/shaders/crt_pixelate.js --format=js",
    "glslx:watch": "npm-watch glslx",
    "inline": "copyfiles --flat dist/webpack/index.html dist/roadroller && inliner dist/roadroller/index.html > dist/index.html",
    "lint": "biome check && eslint",
    "prod": "webpack serve --mode production",
    "roadroller": "mkdirp dist/roadroller && roadroller dist/webpack/bundle.js -o dist/roadroller/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "zip": "advzip -4 -a dist/index.zip dist/index.html && dir dist\\index.zip"
  },
  "version": "1.0.0",
  "watch": {
    "glslx": {
      "extensions": "glslx",
      "patterns": [
        "glslx"
      ],
      "quiet": false
    }
  }
}
